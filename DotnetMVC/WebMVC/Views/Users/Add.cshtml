@model UsersViewModel
@{
    ViewData["Title"] = "Users View Data";
}

<h2>Form Add Users</h2>

<form asp-action="Save" method="post">
    <div class="form-group">
        <label asp-for="Id">Users Id</label>
        <input asp-for="Id" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="SendDate">Send Date</label>
        <input asp-for="SendDate" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="IdHistory">Id History</label>
        <input asp-for="IdHistory" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="IdAudit">Id Audit</label>
        <input asp-for="IdAudit" class="form-control">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>
<a asp-action="List" class="btn btn-sm btn-danger">Back</a>

@section Scripts{
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}