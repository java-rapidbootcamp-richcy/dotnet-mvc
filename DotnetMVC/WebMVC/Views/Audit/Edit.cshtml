@model AuditViewModel
@{
    ViewData["Title"] = "Audit Edit Data";
}

<h2>Form Edit Audit</h2>

<form asp-action="Update" method="post">
    <div class="form-group">
        <label asp-for="Id">Audit Id</label>
        <input asp-for="Id" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="IdAsset">Asset Id</label>
        <input asp-for="IdAsset" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="ConditionAsset">Condition Asset</label>
        <input asp-for="ConditionAsset" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="AntiVirus">Anti Virus</label>
        <input asp-for="AntiVirus" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="WindowsLicense">Windows License</label>
        <input asp-for="WindowsLicense" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="OfficeLicense">Office License</label>
        <input asp-for="OfficeLicense" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Validation">Validation</label>
        <input asp-for="Validation" class="form-control">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>
<a asp-action="List" class="btn btn-sm btn-danger">Back</a>

@section Scripts{
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}