@model List<AuditViewModel>
    @{
    ViewData["Title"] = "Audit List";
    }

    <h2>Asset List</h2>
    <table class="table table-responsive">
        <thead>
            <tr>
                <th>ID Audit</th>
                <th>ID Asset</th>
                <th>Condition Asset</th>
                <th>Anti Virus</th>
                <th>Windows License</th>
                <th>Office License</th>
                <th>Validation</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var item in Model )
    {
            <tr>
                <td>@item.Id</td>
                <td>@item.IdAsset</td>
                <td>@item.ConditionAsset</td>
                <td>@item.AntiVirus</td>
                <td>@item.WindowsLicense</td>
                <td>@item.OfficeLicense</td>
                <td>@item.Validation</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id" class="btn btn-sm btn-warning">Edit</a>
                    <a asp-action="Details" asp-route-id="@item.Id" class="btn btn-sm btn-success">View</a>
                    <a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-sm btn-danger">Delete</a>
                </td>

            </tr>
    }
        </tbody>
    </table>

    <a asp-action="Add" class="btn btn-sm btn-primary">Add</a>

